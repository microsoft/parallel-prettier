{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "moduleResolution": "node",
    "newLine": "LF",
    "module": "commonjs",
    "target": "es6",
    "lib": ["es6", "es7"],
    "outDir": "dist",
    "types": ["node"]
  }
}
